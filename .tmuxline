# This tmux statusbar config was created by tmuxline.vim
# on Wed, 28 Mar 2018

set -g status-justify "centre"
set -g status "on"
set -g status-attr "none"
set -g message-command-bg "colour32"
set -g status-left-length "100"
set -g pane-active-border-fg "colour214"
set -g status-bg "colour32"
set -g message-command-fg "colour232"
set -g pane-border-fg "colour32"
set -g message-bg "colour32"
set -g status-left-attr "none"
set -g status-right-attr "none"
set -g status-right-length "100"
set -g message-fg "colour232"
setw -g window-status-fg "colour15"
setw -g window-status-attr "none"
setw -g window-status-activity-bg "colour32"
setw -g window-status-activity-attr "none"
setw -g window-status-activity-fg "colour214"
setw -g window-status-separator ""
setw -g window-status-bg "colour32"
set -g status-left "#[fg=colour232,bg=colour214] #S #[fg=colour214,bg=colour32,nobold,nounderscore,noitalics]#[fg=colour232,bg=colour32] #F #[fg=colour32,bg=colour32,nobold,nounderscore,noitalics]#[fg=colour15,bg=colour32] #W #[fg=colour32,bg=colour32,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=colour32,bg=colour32,nobold,nounderscore,noitalics]#[fg=colour15,bg=colour32] %a #[fg=colour32,bg=colour32,nobold,nounderscore,noitalics]#[fg=colour232,bg=colour32] %b %d  %R #[fg=colour214,bg=colour32,nobold,nounderscore,noitalics]#[fg=colour232,bg=colour214] #H "
setw -g window-status-format "#[fg=colour32,bg=colour32,nobold,nounderscore,noitalics]#[default] #I  #W #[fg=colour32,bg=colour32,nobold,nounderscore,noitalics]"
setw -g window-status-current-format "#[fg=colour32,bg=colour32,nobold,nounderscore,noitalics]#[fg=colour232,bg=colour32] #I  #W #[fg=colour32,bg=colour32,nobold,nounderscore,noitalics]"
